{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/no-typos": "off",
        "react/require-default-props": "off",
        "import/prefer-default-export": "off",
        "no-plusplus": "off",
        "no-underscore-dangle": "off",
        "react/sort-comp": "off",
        "no-console": "off",
        "no-confusing-arrow": "off"
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "parser": "babel-eslint"
}